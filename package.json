        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "rollup": "^2.56.3",
        "rollup-plugin-copy": "^3.4.0",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-postcss": "^3.1.8",
        "rollup-plugin-typescript2": "^0.29.0",
        "sass-loader": "^10.2.0",
        "typescript": "^4.4.2"
    },
    "dependencies": {
        "update": "^0.7.4"
